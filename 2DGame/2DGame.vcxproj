    <PreBuildEvent>
      <Command>@echo on

echo "Check Libs"

if not exist "$(SolutionDir)libs" mkdir "$(SolutionDir)libs"

if not exist "$(SolutionDir)libs\tmp" mkdir "$(SolutionDir)libs\tmp"

echo "Check SFML"
if exist $(SolutionDir)libs\SFML-2.5.1 (
  echo "SFML is already existing"
) else (
 echo "Download SFML"
 powershell -Command "Invoke-WebRequest https://www.sfml-dev.org/files/SFML-2.5.1-windows-vc15-64-bit.zip -Outfile $(SolutionDir)libs\tmp\TMP.zip"
 powershell Expand-Archive $(SolutionDir)libs\tmp\TMP.zip -DestinationPath $(SolutionDir)libs\
)

xcopy /y "$(SolutionDir)libs\SFML-2.5.1\bin\openal32.dll" "$(SolutionDir)x64\Release Windows\"

xcopy /y "$(SolutionDir)libs\SFML-2.5.1\bin\sfml-audio-2.dll" "$(SolutionDir)x64\Release Windows\"

xcopy /y "$(SolutionDir)libs\SFML-2.5.1\bin\sfml-graphics-2.dll" "$(SolutionDir)x64\Release Windows\"

xcopy /y "$(SolutionDir)libs\SFML-2.5.1\bin\sfml-network-2.dll" "$(SolutionDir)x64\Release Windows\"

xcopy /y "$(SolutionDir)libs\SFML-2.5.1\bin\sfml-system-2.dll" "$(SolutionDir)x64\Release Windows\"

xcopy /y "$(SolutionDir)libs\SFML-2.5.1\bin\sfml-window-2.dll" "$(SolutionDir)x64\Release Windows\"

echo "Check IMGUI"
if exist $(SolutionDir)libs\imgui-master (
  echo "SFML is already existing"
) else (
 echo "Download IMGUI"
 powershell -Command "Invoke-WebRequest https://github.com/ocornut/imgui/archive/refs/heads/master.zip -Outfile $(SolutionDir)libs\tmp\TMP.zip"
 powershell Expand-Archive $(SolutionDir)libs\tmp\TMP.zip -DestinationPath $(SolutionDir)libs\
)

echo "Check IMGUI SFML"
if exist $(SolutionDir)libs\imgui-sfml-master (
  echo "SFML is already existing"
) else (
 echo "Download IMGUI SFML"
 powershell -Command "Invoke-WebRequest https://github.com/eliasdaler/imgui-sfml/archive/refs/heads/master.zip -Outfile $(SolutionDir)libs\tmp\TMP.zip"
 powershell Expand-Archive $(SolutionDir)libs\tmp\TMP.zip -DestinationPath $(SolutionDir)libs\
)

echo "Check RapidXML"
if exist $(SolutionDir)libs\rapidxml-master (
  echo "SFML is already existing"
) else (
 echo "Download RapidXML"
 powershell -Command "Invoke-WebRequest https://github.com/discord/rapidxml/archive/refs/heads/master.zip -Outfile $(SolutionDir)libs\tmp\TMP.zip"
 powershell Expand-Archive $(SolutionDir)libs\tmp\TMP.zip -DestinationPath $(SolutionDir)libs\
)


if not exist "$(OutDir)data" mkdir "$(OutDir)data"
xcopy /s /d /y "$(ProjectDir)data" "$(OutDir)data"
</Command>
